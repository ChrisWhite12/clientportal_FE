     setCount(count + 1)\n    }\n\n    const handleAccept = (event) => {\n        let ticketUpdate = tickets.filter(el => el._id == event.target.value)[0]\n        ticketUpdate.status = 'accepted'\n        updateTicket(ticketUpdate)\n        .then((res) => console.log(res))\n        .catch((err) => console.log(err))\n    }\n\n    const handleReject = (event) => {\n        let ticketUpdate = tickets.filter(el => el._id == event.target.value)[0]\n        ticketUpdate.status = 'rejected'\n        updateTicket(ticketUpdate)\n        .then((res) => console.log(res))\n        .catch((err) => console.log(err))\n    }\n\n    const handleDelete = (event) => {\n        deleteTicket(event.target.value)\n        .then((res) => console.log(res))\n        .catch((err) => console.log(err))\n    }\n\n    return (\n        <div>\n            <h1>Notifications</h1>\n            <ul>\n            {tickets.map((ticket) => {\n                return <li key={ticket._id}><p>Ticket: {ticket._id}</p><p>appId:{ticket.appId}</p><p>appDate: {ticket.appDate}</p> \n                <p>status: {ticket.status}</p><p>userId: {ticket.userId}</p><p>pracId:{ticket.practitionerId}</p>\n                <button onClick={(event) => handleReject(event)} value={ticket._id}>Reject</button>\n                <button onClick={(event) => handleAccept(event)} value={ticket._id}>Accept</button>\n                <button onClick={(event) => handleDelete(event)} value={ticket._id}>Delete</button></li>\n            })}\n            </ul>\n            <button id=\"create_ticket\" onClick={clickTicket}>\n                    Create Ticket\n            </button>\n        </div>\n    )\n}\n\nexport default Notifications","/home/chrisw/projects/Term3/MERN_Assesment/client_portal_app/src/components/Login.js",["162","163"],"import React, {Component, useState} from 'react'\nimport {useGlobalState} from \"../config/store\"\nimport {loginUser} from \"../services/authServices\"\nimport {Link} from 'react-router-dom'\n\nconst Login = ({history}) => {\n    const [userDetails, setUserDetails] = useState()\n    const [message,setMessage] = useState('')\n    const {store,dispatch} = useGlobalState()\n\n    function handleChange(event){\n        const name = event.target.name\n        const value = event.target.value\n\n        setUserDetails({\n            ...userDetails,\n            [name]: value\n        })\n        // console.log(userDetails)\n    }\n\n    function handleSubmit(event){\n        event.preventDefault()\n        loginUser(userDetails)\n            .then(() => {\n                dispatch({\n                    type: \"setLoggedInUser\",\n                    data: userDetails.email\n                })\n                history.push('/dashboard')\n            })\n            .catch((err) => {\n                setMessage('Invalid email or password')\n            })\n    }\n\n    return (\n        <div className=\"main_sec\">\n            <h1>Welcome to the Brain Train Client Portal</h1>\n            <form className=\"login_card\" onSubmit={handleSubmit}>\n                <input className=\"login_fields\" name=\"email\" onChange={handleChange} type=\"text\" placeholder=\"email\"></input>\n                <input className=\"login_fields\" name=\"password\" onChange={handleChange} type=\"password\" placeholder=\"password\"></input>\n                <p>{message}</p>\n                <div>\n                    <input type=\"checkbox\"></input>\n                    <label> Remember Me</label>\n                </div>\n                <Link to=\"/forgot_password\">\n                    Forgot password\n                </Link>\n                <input id=\"login_btn\" type=\"submit\" value=\"Log In\"></input>\n                <input id=\"sign_up_btn\" type=\"button\" value=\"Sign Up\"></input>\n            </form>\n        </div>\n    )\n}\n\nexport default Login",["164","165"],"/home/chrisw/projects/Term3/MERN_Assesment/client_portal_app/src/components/FormContainer.js",[],"/home/chrisw/proje